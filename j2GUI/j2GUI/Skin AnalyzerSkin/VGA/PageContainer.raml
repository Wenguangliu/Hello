<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	x:Class="Resco.Controls.PageContainer"
	Height="9">
		<WrapPanel x:Name="LayoutRoot" Margin="1" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
			<Image x:Name="PageOn" Margin="1,0" Width="7" Height="7" Source="Images\page_on.png" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="None"/>
			<Image x:Name="PageOff" Margin="1,0" Width="7" Height="7" Source="Images\page_off.png" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="None"/>
		</WrapPanel>
	</UserControl>